<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>InsertCustomerServlet</servlet-name>
        <servlet-class>com.crud.dressrental.InsertCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCustomerServlet</servlet-name>
        <servlet-class>com.crud.dressrental.EditCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditCustomersServlet</servlet-name>
        <servlet-class>com.crud.dressrental.EditCustomersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteCustomersServlet</servlet-name>
        <servlet-class>com.crud.dressrental.DeleteCustomersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertRentalServlet</servlet-name>
        <servlet-class>com.crud.dressrental.InsertRentalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditRentalServlet</servlet-name>
        <servlet-class>com.crud.dressrental.EditRentalServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteRentalServlet</servlet-name>
        <servlet-class>com.crud.dressrental.DeleteRentalServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>InsertCustomerServlet</servlet-name>
        <url-pattern>/InsertCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCustomerServlet</servlet-name>
        <url-pattern>/EditCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditCustomersServlet</servlet-name>
        <url-pattern>/EditCustomersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteCustomersServlet</servlet-name>
        <url-pattern>/DeleteCustomersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertRentalServlet</servlet-name>
        <url-pattern>/InsertRentalServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditRentalServlet</servlet-name>
        <url-pattern>/EditRentalServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteRentalServlet</servlet-name>
        <url-pattern>/DeleteRentalServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
